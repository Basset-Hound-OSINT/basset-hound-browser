{
  "name": "my-custom-browser",
  "version": "1.0.0",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "watch": "nodemon --exec electron ."
  },
  "dependencies": {
    "electron": "^28.0.0",
    "electron-context-menu": "^3.6.1",
    "electron-store": "^8.1.0",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "electron-builder": "^24.6.3",
    "nodemon": "^3.0.2"
  },
  "build": {
    "appId": "com.example.mybrowser",
    "productName": "My Custom Browser",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "extensions/**/*"
    ]
  }
}